<div id="experience" class='bg-blue-100 overflow-hidden relative'>
  <main
    class='mt-10 mx-auto max-w-7xl px-4 sm:mt-12 sm:px-6 md:mt-16 lg:mt-20 lg:px-8 xl:mt-28'
  >
    <div class='sm:text-center lg:text-left'>
      <h1
        class='text-4xl tracking-tight font-extrabold text-gray-900 sm:text-5xl md:text-6xl'
      >
        <span class='block xl:block'>Prácovne skúsenosti</span>
      </h1>

      <div class="my-8 sm:my-8 xl:my-16">
          <div class="md:grid md:grid-cols-3 md:gap-12">
              <div class="mt-5 md:mt-0 md:col-span-2"> 
                {{#each jobs}}
                {{>subpartials/job position=this.position employer=this.employer started=this.started ended=this.ended isCurrent=this.isCurrent location=this.location body=this.body}}
                {{/each}}
              </div>
          </div>
      </div>
    </div>
  </main>
</div>